contract GameList =

  record state = {
    owner    : address,
    total_supply     : int,
    games    : map(int, address)}
  
  stateful entrypoint init() = {
    owner = Call.caller,
    total_supply = 0,
    games = {}}
 
  public entrypoint getGamesList() : map(int, address) =
    state.games
  
  stateful entrypoint addGame(g: address) =
    put(state{
      games[state.total_supply] = g,
      total_supply = state.total_supply + 1
      })
    state.total_supply
